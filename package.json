{
  "name": "manazel-alabrar-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "npm run translate",
    "build": "ng build --configuration production ",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "translate": "cd translations && node translate-generate.js ",
    "deploy": "npm run translate && ng build  --configuration production  --base-href /manazel-alabrar-fe/ && cp dist/manazel-alabrar-fe/index.html dist/manazel-alabrar-fe/404.html",
    "analyzer": "ng build  --configuration production --build-optimizer --stats-json && webpack-bundle-analyzer dist/manazel-alabrar-fe/stats.json",
    "serve:ssr:manazel-alabrar-fe": "node dist/manazel-alabrar-fe/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.9",
    "@angular/cdk": "^17.0.5",
    "@angular/common": "^17.0.9",
    "@angular/compiler": "^17.0.9",
    "@angular/core": "^17.0.9",
    "@angular/forms": "^17.0.9",
    "@angular/material": "^17.0.5",
    "@angular/platform-browser": "^17.0.9",
    "@angular/platform-browser-dynamic": "^17.0.9",
    "@angular/platform-server": "^17.0.9",
    "@angular/router": "^17.0.9",
    "@angular/service-worker": "^17.0.9",
    "@angular/ssr": "^17.0.10",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@types/marked": "^4.3.0",
    "express": "^4.18.2",
    "jquery": "^3.6.1",
    "marked": "^5.0.0",
    "ngx-extended-pdf-viewer": "^19.0.0",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.4.8",
    "swiper": "^9.2.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.10",
    "@angular/cli": "~17.0.10",
    "@angular/compiler-cli": "^17.0.9",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.18.0",
    "csv-parser": "^3.0.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.8.4",
    "typescript": "~5.2.2",
    "webpack-bundle-analyzer": "^4.8.0"
  }
}
