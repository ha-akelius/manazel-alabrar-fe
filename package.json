{
  "name": "manazel-alabrar-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "npm run translate",
    "build": "ng build --configuration production ",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "translate": "cd translations && node translate-generate.js ",
    "deploy": "npm run translate && ng build  --configuration production  --base-href /manazel-alabrar-fe/ && cp dist/manazel-alabrar-fe/index.html dist/manazel-alabrar-fe/404.html",
    "analyzer": "ng build  --configuration production --build-optimizer --stats-json && webpack-bundle-analyzer dist/manazel-alabrar-fe/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.1",
    "@angular/cdk": "^16.2.1",
    "@angular/common": "^16.2.1",
    "@angular/compiler": "^16.2.1",
    "@angular/core": "^16.2.1",
    "@angular/forms": "^16.2.1",
    "@angular/material": "^16.2.1",
    "@angular/platform-browser": "^16.2.1",
    "@angular/platform-browser-dynamic": "^16.2.1",
    "@angular/router": "^16.2.1",
    "@angular/service-worker": "^16.2.1",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@types/marked": "^5.0.1",
    "deep-copy-ts": "^0.5.4",
    "jquery": "^3.6.1",
    "marked": "^7.0.4",
    "ngx-extended-pdf-viewer": "^18.0.0-beta.3",
    "rxjs": "~7.8.1",
    "sweetalert2": "^11.7.27",
    "swiper": "^10.2.0",
    "tslib": "^2.6.2",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular/cli": "~16.2.1",
    "@angular/compiler-cli": "^16.2.1",
    "@types/jasmine": "~4.3.5",
    "csv-parser": "^3.0.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.2",
    "typescript": "~5.1.6",
    "webpack-bundle-analyzer": "^4.9.0"
  }
}
