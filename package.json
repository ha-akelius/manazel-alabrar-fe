{
  "name": "manazel-alabrar-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "admin-ar": "ng s admin --configuration=ar",
    "i18n-admin": "ng extract-i18n --project admin",
    "prebuild-admin": "npm run prisma:generate",
    "build-admin": "ng build  --project admin --localize",
    "build-site": "ng build --configuration production",
    "prebuild-site": "npm run translate && npm run postprebuild",
    "postprebuild": "prettier ./src/app/core/models --write && prettier ./src/assets/i18n --write ",
    "build": "npm run build-site && npm run build-admin",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "preprisma:generate": "npm run build --prefix prisma-idt-generator && npm i ./prisma-idt-generator",
    "prisma:generate": "prisma generate && npx prettier projects/admin/src/core/services/api.service.ts --write && npx prettier projects/admin/src/models/**/* --write && npx prettier projects/admin/src/app/translations/ --write",
    "translate": "cd translations && node translate-generate.js ",
    "deploy": "npm run translate && ng build  --configuration production  --base-href /manazel-alabrar-fe/ && cp dist/manazel-alabrar-fe/index.html dist/manazel-alabrar-fe/404.html",
    "analyzer": "ng build  --configuration production --build-optimizer --stats-json && webpack-bundle-analyzer dist/manazel-alabrar-fe/stats.json",
    "serve:ssr:manazel-alabrar-fe": "node dist/manazel-alabrar-fe/server/server.mjs",
    "lint": "ng lint",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.4",
    "@angular/cdk": "^17.2.2",
    "@angular/common": "^17.2.4",
    "@angular/compiler": "^17.2.4",
    "@angular/core": "^17.2.4",
    "@angular/forms": "^17.2.4",
    "@angular/material": "^17.3.7",
    "@angular/platform-browser": "^17.2.4",
    "@angular/platform-browser-dynamic": "^17.2.4",
    "@angular/platform-server": "^17.2.4",
    "@angular/router": "^17.2.4",
    "@angular/service-worker": "^17.2.4",
    "@angular/ssr": "^17.0.10",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@prisma/client": "^5.10.2",
    "@types/json-schema": "^7.0.15",
    "@types/marked": "^4.3.0",
    "express": "^4.18.2",
    "jquery": "^3.6.1",
    "marked": "^5.0.0",
    "ngx-extended-pdf-viewer": "^19.0.0",
    "prisma-idt-generator": "file:prisma-idt-generator",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.4.8",
    "swiper": "^9.2.0",
    "tslib": "^2.3.0",
    "zod": "^3.23.4",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.3",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "~17.2.3",
    "@angular/compiler-cli": "^17.2.4",
    "@angular/localize": "^17.2.4",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "prisma-json-schema-generator": "^5.1.1",
    "typescript": "~5.3.3",
    "webpack-bundle-analyzer": "^4.8.0"
  }
}
